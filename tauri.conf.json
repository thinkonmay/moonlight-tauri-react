{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devPath": "http://localhost:3000",
    "distDir": "../dist"
  },
  "package": {
    "productName": "Thinkmay",
    "version": "0.1.13"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "http": {
        "all": true,
        "request": true,
        "scope": [
          "http://**",
          "https://**"
        ]
      },
      "shell": {
        "scope": [
          {
            "name": "Moonlight",
            "cmd": "./moonlight/Moonlight.exe",
            "args": true
          },
          {
            "name": "Daemon",
            "cmd": "./daemon.exe",
            "args": true
          },
          {
            "name": "ZeroTier",
            "cmd": "zerotier-cli",
            "args": true
          }
        ]
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/icon.ico",
        "icons/icon.png"
      ],
      "identifier": "thinkmay.net",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://gist.githubusercontent.com/epitchi/85b62c4b7084ed8e9818992ef2501102/raw/"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEY1MUY2RDU4OTdCRjY3OEIKUldTTFo3K1hXRzBmOVVlV1BpY2tKclNzQ2cwZXBZalNOeEJOa09DYmdpMzlpaTdFbWZiK3NqbmoK",
      "windows": {
        "installMode": "passive"
      }
    },
    "windows": [
      {
        "fullscreen": true,
        "height": 600,
        "resizable": true,
        "title": "Thinkmay",
        "width": 800
      }
    ]
  }
}